<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>Getting Started</title>
  <meta name="description" content="">
  <meta name="author" content="">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  
  <%= javascript_include_tag "libs/modernizr" %>
  <%= stylesheet_link_tag "application" %>

  <!-- Typekit -->
  <script type="text/javascript" src="//use.typekit.net/ksh5vfs.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

</head>
<body class="<%= page_classes %> <%= data.page.page_classes %>">
  <header class="site-header">
  </header>
  <!-- /site-header -->
  
  <section class="main-content">
    <%= yield %>
  </section><!-- /main-content -->

  <%= partial "partials/site-footer" %>
  
  <!-- 4. Check for Zepto support, load jQuery if necessary -->
  <script>
    document.write('<script src=/javascripts/vendor/'
        + ('__proto__' in {} ? 'zepto' : 'jquery')
        + '.js><\/script>');
  </script>
  <%= javascript_include_tag "public" %>


</body>
</html>
